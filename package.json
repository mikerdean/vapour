    "eslint-plugin-solid": "0.13.0",
    "jsdom": "22.1.0",
    "mock-socket": "9.3.1",
    "postcss": "8.4.31",
    "prettier": "3.1.0",
    "tailwindcss": "3.3.5",
    "typescript": "5.2.2",
    "vite": "4.5.0",
    "vite-plugin-solid": "2.7.2",
    "vitest": "0.34.6"
  },
  "volta": {
    "node": "18.18.2"
  }
