{
  "name": "vapour",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "build": "npm run typecheck && vite build",
    "lint": "npm run typecheck && npm run lint:eslint && npm run lint:prettier",
    "lint:eslint": "eslint .",
    "lint:prettier": "prettier --check .",
    "preview": "vite preview",
    "test": "NODE_NO_WARNINGS=1 vitest",
